{
  "name": "fork-sync-protection",
  "version": "1.0.0",
  "description": "Fork Sync Protection System for maintaining auto-accept-countdown feature integrity",
  "main": "workflow-coordinator.ts",
  "scripts": {
    "start": "npx ts-node workflow-coordinator.ts",
    "check": "npx ts-node workflow-coordinator.ts --check-only",
    "auto-merge": "npx ts-node workflow-coordinator.ts --auto-merge",
    "monitor": "npx ts-node monitor.ts",
    "security": "npx ts-node security-validator.ts",
    "e2e": "npx ts-node e2e-validator.ts",
    "test": "npm run e2e",
    "validate": "npm run security && npm run e2e"
  },
  "dependencies": {
    "@types/node": "^18.0.0",
    "typescript": "^5.0.0",
    "ts-node": "^10.9.0"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bizzkoot/TimerCC.git"
  },
  "keywords": [
    "fork-sync",
    "protection",
    "github-actions",
    "automation",
    "claude-code",
    "typescript"
  ],
  "author": "Muhammad Faiz",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "*.ts",
    "package.json",
    "README.md"
  ]
}